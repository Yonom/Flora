{
  "expo": {
    "name": "flora-app",
    "slug": "flora-app",
    "version": "1.0.0",
    "scheme": "com.yonom.flora",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yonom.flora",
      "runtimeVersion": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.CAMERA"],
      "package": "com.yonom.flora",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "To scan food items, flora needs access to your camera"
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://flora-backend.simonfarshid.com/"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://flora-backend.simonfarshid.com/"
      },
      "eas": {
        "projectId": "04be704b-9f8d-4167-8c2b-129291050047"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/04be704b-9f8d-4167-8c2b-129291050047"
    }
  }
}
